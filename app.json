{
  "expo": {
    "name": "Kehad-App",
    "slug": "Kehad-App",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/kehadLogo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets//KehadSplash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          // "icon": "./local/assets/icon.png",
          "icon": "./local/assets/kehadLogo.png",
          "color": "#ffffff",
          "defaultChannel": "default"
        }
      ]
    ]
  }
}

